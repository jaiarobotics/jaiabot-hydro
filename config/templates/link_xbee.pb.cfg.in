link {
    modem_id: $modem_id
    subnet_mask: $subnet_mask
    driver { 
        # returned by goby_driver_name() function in shared library listed in GOBY_MODEMDRIVER_PLUGINS
        driver_name: "xbee_driver"
        serial_port: "$serial_port"
        serial_baud: 9600
        modem_id: $modem_id
        [xbee.protobuf.config] {
            network_id: $fleet_id
            fleet_id: $fleet_id
            use_xbee_encryption: $use_encryption
            xbee_encryption_password: "$encryption_password"
            $xbee_hub_id
        }
    }
    mac { 
        type: MAC_FIXED_DECENTRALIZED
        $mac_slots
    }

    ack_timeout: $ack_timeout
    # read from _sub_buffer.pb.cfg.in
    $sub_buffer
}
